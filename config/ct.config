[
    {hpr, [
        {aws_config, [
            {access_key_id, "testkey"},
            {secret_access_key, "testsecret"},
            {aws_region, "us-east-1"}
        ]}
    ]},
    {lager, [
        {suppress_supervisor_start_stop, true},
        {log_root, "log/hpr"},
        {crash_log, "crash.log"},
        {handlers, [
            {lager_console_backend, [
                {level, info},
                {formatter_config, [
                    "time=",
                    date,
                    "T",
                    time,
                    " lvl=",
                    severity_upper,
                    " ",
                    metadata,
                    " msg=\"",
                    message,
                    "\"",
                    "\n"
                ]}
            ]},
            {lager_file_backend, [
                {file, "info.log"},
                {level, info},
                {size, 0},
                {date, "$D0"},
                {count, 7},
                {formatter_config, [
                    "time=",
                    date,
                    "T",
                    time,
                    " lvl=",
                    severity_upper,
                    " ",
                    metadata,
                    " msg=\"",
                    message,
                    "\"",
                    "\n"
                ]}
            ]}
        ]}
    ]}
].
